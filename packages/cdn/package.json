{
  "name": "@wikisophia/api-client",
  "version": "0.6.0",
  "browser": "dist",
  "description": "Wikisophia API javascript client",
  "scripts": {
    "clean": "rm -rf dist",
    "build:css": "node bundle-css.js",
    "build:dev": "concurrently \"rollup -c rollup.config.dev.js\" \"npm run build:css\"",
    "build:release": "NODE_ENV=production rollup -c rollup.config.prod.js",
    "lint": "eslint src --fix && stylelint \"src/**/*.css\"",
    "lint:nofix": "eslint src && stylelint \"src/**/*.css\"",
    "serve:dev": "concurrently \"npm run watch:dev\" \"http-server -p 4041 ./dist\"",
    "serve:release": "concurrently \"NODE_ENV=production npm run watch:release\" \"http-server -p 4041 ./dist\"",
    "test": "BABEL_ENV=test jest",
    "test-coverage": "BABEL_ENV=test jest -c jest.config.coverage.js",
    "watch:css": "nodemon -e css --watch src -x \"npm run build:css\"",
    "watch:dev": "concurrently \"rollup -c rollup.config.dev.js -w\" \"npm run watch:css\"",
    "watch:release": "concurrently \"NODE_ENV=production rollup -c rollup.config.prod.js -w\" \"npm run watch:css\""
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@wikisophia/api-arguments-client": "^0.4.1",
    "babel-jest": "^24.1.0",
    "babelrc-rollup": "^3.0.0",
    "clean-css": "^4.2.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "fs-extra": "^7.0.1",
    "http-server": "^0.11.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "postcss-syntax": "^0.36.2",
    "react": "^16.8.6",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-uglify": "^6.0.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "xhr-mock": "^2.4.1"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikisophia/api-js-client.git"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/wikisophia/api-js-client/issues"
  },
  "homepage": "https://github.com/wikisophia/api-js-client#readme"
}
