{
  "name": "@wikisophia/ui-server",
  "version": "0.1.0",
  "description": "A UI server which powers Wikisophia",
  "scripts": {
    "build": "babel src -d build",
    "serve:dev": "nodemon --exec babel-node src/server.js",
    "serve:prod": "npm run build && node build/server.js",
    "lint": "eslint src --fix",
    "lint:nofix": "eslint src"
  },
  "dependencies": {
    "@wikisophia/api-arguments-client": "^0.7.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-validator": "^6.1.1",
    "node-fetch": "^2.6.0",
    "moment": "^2.24.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1"
  },
  "keywords": [
    "wikisophia"
  ],
  "author": "Dave <dave@wikisophia.net>",
  "license": "GPL-3.0-only"
}
