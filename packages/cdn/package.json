{
  "name": "@wikisophia/api-client",
  "version": "0.6.0",
  "browser": "dist",
  "description": "Wikisophia API javascript client",
  "scripts": {
    "build:css": "node bundle-css.js",
    "build:dev": "webpack --config=./webpack.dev.js",
    "build:prod": "webpack --config=./webpack.prod.js",
    "clean": "rm -rf dist",
    "dev": "concurrently \"webpack-dev-server --config=./webpack.dev.js\" \"npm run watch:css\"",
    "lint": "eslint src --ext js,jsx --fix && stylelint \"src/**/*.css\"",
    "lint:nofix": "eslint src --ext js,jsx && stylelint \"src/**/*.css\"",
    "prod": "concurrently \"webpack-dev-server --config=./webpack.prod.server.js\" \"npm run watch:css\"",
    "test": "BABEL_ENV=test jest --passWithNoTests",
    "test-coverage": "BABEL_ENV=test jest -c jest.config.coverage.js --passWithNoTests",
    "watch:css": "nodemon -e css --watch src -x \"npm run build:css\""
  },
  "dependencies": {
    "@wikisophia/api-arguments-client": "^0.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-jest": "^24.1.0",
    "clean-css": "^4.2.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "postcss-syntax": "^0.36.2",
    "regenerator-runtime": "^0.13.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.1.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1",
    "xhr-mock": "^2.4.1"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikisophia/api-js-client.git"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/wikisophia/api-js-client/issues"
  },
  "homepage": "https://github.com/wikisophia/api-js-client#readme"
}
