{
  "name": "@wikisophia/ui-server",
  "version": "0.1.0",
  "description": "A UI server which powers Wikisophia",
  "scripts": {
    "build": "babel src -d build",
    "serve:dev": "nodemon --exec babel-node src/server.js",
    "serve:prod": "npm run build && node build/server.js",
    "lint": "eslint src --fix",
    "lint:nofix": "eslint src"
  },
  "dependencies": {
    "@wikisophia/api-arguments-client": "^0.6.0",
    "body-parser": "^1.18.3",
    "express": "^4.17.1",
    "express-handlebars": "^3.0.2",
    "express-validator": "^5.3.1",
    "node-fetch": "^2.3.0",
    "moment": "^2.24.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.18.11"
  },
  "keywords": [
    "wikisophia"
  ],
  "author": "Dave <dave@wikisophia.net>",
  "license": "GPL-3.0-only"
}
